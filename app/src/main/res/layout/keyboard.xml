<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/colorPrimary"
    android:id="@+id/ll1"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_20"
        android:textSize="@dimen/sp_18"
        android:textColor="#333333"
        android:text="请输入“林志玲”的手机号码"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_50"
        android:layout_marginRight="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_60">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:textColor="#333333"
            android:text="国家/地区"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/dp_50"
            android:textColor="#333333"
            android:text="中国"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_50"
        android:layout_marginRight="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_90">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="+86"/>
        <EditText
            android:id="@+id/evinput"
            android:layout_width="@dimen/dp_150"
            android:layout_height="wrap_content"
            android:background="@null"
            android:textColor="#333333"
            android:layout_marginLeft="@dimen/sp_20"
            android:hint="请输入手机号码"/>
        <TextView
            android:layout_width="@dimen/dp_50"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#B14641"
            android:textSize="@dimen/dp_16"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="通讯录"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_50"
        android:layout_marginRight="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_150">
        <TextView
            android:id="@+id/tv_cancel"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_40"
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="#333333"
            android:textSize="@dimen/sp_18"
            android:text="取消"/>
        <TextView
            android:id="@+id/invitation"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_40"
            android:gravity="center"
            android:layout_weight="1"
            android:background="#B14641"
            android:layout_marginLeft="@dimen/dp_40"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:text="邀请"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_210">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/numberkey"
                android:layout_width="@dimen/dp_270"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv1"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:keyboardNavigationCluster="true"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="1"/>
                <TextView
                    android:id="@+id/tv2"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:gravity="center"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:text="2"/>
                <TextView
                    android:id="@+id/tv3"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:gravity="center"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:text="3"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="@dimen/dp_270"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv4"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="4"/>
                <TextView
                    android:id="@+id/tv5"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="5"/>
                <TextView
                    android:id="@+id/tv6"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="6"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="@dimen/dp_270"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv7"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="7"/>
                <TextView
                    android:id="@+id/tv8"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="8"/>
                <TextView
                    android:id="@+id/tv9"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="9"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="@dimen/dp_270"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv10"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="."/>
                <TextView
                    android:id="@+id/tv0"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_25"
                    android:gravity="center"
                    android:text="0"/>
                <TextView
                    android:id="@+id/dismiss"
                    android:layout_width="@dimen/dp_90"
                    android:layout_height="@dimen/dp_40"
                    android:textColor="#2A2B2C"
                    android:textSize="@dimen/sp_15"
                    android:gravity="center"
                    android:text="开关"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/shanchu"
                android:layout_width="@dimen/dp_90"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="@dimen/dp_21"
                android:text="删除"/>
            <TextView
                android:id="@+id/queding"
                android:layout_width="@dimen/dp_90"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#108EE9"
                android:textSize="@dimen/dp_21"
                android:textColor="#FFFFFF"
                android:gravity="center"
                android:text="确定"/>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>